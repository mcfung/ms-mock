{
  "name": "Server 1",
  "port": 5020,
  "config": [
    {
      "static": true,
      "path": "/Users/manhochung/IdeaProjects/sourcing-frontend/mock"
    },
    {
      "path": "/preference/v1/me/preference",
      "method": "options",
      "combinations": [
        {
          "cors": true,
          "response": {
            "headers": [
              {
                "name": "access-control-allow-credentials",
                "value": "true"
              },
              {
                "name": "access-control-allow-headers",
                "value": "DNT,X-CustomHeader,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,X-SAT,Authorization"
              },
              {
                "name": "access-control-allow-methods",
                "value": "DELETE, GET, POST, OPTIONS"
              },
              {
                "name": "access-control-allow-origin",
                "value": "https://sourcing-dev.hktdc.com:3000"
              }
            ],
            "statusCode": 204
          }
        }
      ]
    },
    {
      "path": "/preference/v1/me/preference",
      "method": "get",
      "combinations": [
        {
          "cors": true,
          "response": {
            "headers": [
              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "content": {
              "interestedProductService": [
                "1"
              ],
              "interestedRegion": [
                "1"
              ],
              "interestedTopic": [
                "1"
              ]
            },
            "statusCode": 200
          }
        }
      ]
    },
    {
      "path": "/preference/v1/recommended-entities",
      "method": "get",
      "combinations": [
        {
          "cors": true,
          "response": {
            "headers": [
              {
                "name": "Content-Type",
                "value": "application/json"
              }
            ],
            "content": {
              "dfcPathIds": [
                "27",
                "30"
              ],
              "events": [],
              "magazines": []
            },
            "statusCode": 200
          }
        }
      ]
    }
  ]
}